    "                     \n",
    "                     \n",
    "                    \n",
    "nu_mu_to_nu_e = []\n",
    "nu_mu_to_nu_mu = []\n",
    "nu_mu_to_nu_tau = []\n",
    "nu_mu_to_nu_s = []\n",
    "for Enu in energy_value:\n",
    "    nuSQ.Set_E(Enu*units.GeV)\n",
    "    nuSQ.Set_initial_state(np.array([0.,1.,0.,0.]),nsq.Basis.flavor)\n",
    "    nuSQ.EvolveState()\n",
    "    nu_mu_to_nu_e.append(nuSQ.EvalFlavor(0))\n",
